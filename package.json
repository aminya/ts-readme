{
  "name": "ts-readme",
  "version": "1.1.3",
  "description": "Generate docs from typescript + jsdoc and put it in a README",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist",
  "author": "Andrew Lisowski lisowski54@gmail.com",
  "sideEffects": false,
  "contributors": [
    "Andrew Lisowski lisowski54@gmail.com"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "bin": "bin/ts-readme.js",
  "scripts": {
    "build": "ds build",
    "start": "ds build --watch",
    "test": "ds test",
    "lint": "ds lint",
    "docs": "./bin/ts-readme.js --header-depth 4 --matcher TS-README-GENERATED"
  },
  "files": [
    "dist",
    "bin",
    "!*.test.*",
    "!__snapshots__",
    "!__tests__"
  ],
  "dependencies": {
    "command-line-application": "^0.10.1",
    "fast-glob": "^3.3.2",
    "prettier": "3.3.3"
  },
  "peerDependencies": {
    "typescript": ">= 3.x"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "11.2.0",
    "@auto-it/first-time-contributor": "11.2.0",
    "@design-systems/cli": "4.15.4",
    "@types/prettier": "^3.0.0",
    "auto": "11.2.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "typescript": "5.5.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor"
    ]
  }
}
